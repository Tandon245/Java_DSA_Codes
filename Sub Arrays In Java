// Sub Arrays In Java--->>





public class Main
{
	public static void main(String[] args) {
		
		
		int arr[]={1,2,3,4,5,6}; //Array
	
		int n=arr.length; // Size Of The Array
		
		for(int i=0;i<n;i++){
		    for(int j=i;j<n;j++){
		        for(int k=i;k<=j;k++){
		            System.out.print(arr[k]+" ");
		        }
		        System.out.println();
		    }
		}
		
	}
}
